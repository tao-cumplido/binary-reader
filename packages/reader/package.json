{
	"name": "@nishin/reader",
	"version": "0.3.1",
	"description": "Read various data types from a byte array in a streamlined way",
	"keywords": [
		"binary",
		"data",
		"reader",
		"hex",
		"javascript",
		"typescript",
		"esm"
	],
	"license": "ISC",
	"author": "Tao Cumplido",
	"repository": "https://github.com/tao-cumplido/binary-reader/tree/main/packages/reader",
	"bugs": "https://github.com/tao-cumplido/binary-reader/issues",
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/esm/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"require": {
				"types": "./dist/cjs/index.d.cts",
				"default": "./dist/cjs/index.cjs"
			}
		},
		"./data": {
			"import": {
				"types": "./dist/esm/data/index.d.ts",
				"default": "./dist/esm/data/index.js"
			},
			"require": {
				"types": "./dist/esm/data/index.d.cts",
				"default": "./dist/cjs/data/index.cjs"
			}
		},
		"./assert": {
			"import": {
				"types": "./dist/esm/assert/index.d.ts",
				"default": "./dist/esm/assert.js"
			},
			"require": {
				"types": "./dist/cjs/assert/index.d.cts",
				"default": "./dist/cjs/assert.cjs"
			}
		}
	},
	"types": "./dist/cjs/index.d.cts",
	"main": "./dist/cjs/index.cjs",
	"scripts": {
		"build": "tsc",
		"test": "ava"
	},
	"dependencies": {
		"@nishin/enum": "~0.1.0"
	}
}
