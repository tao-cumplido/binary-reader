{
	"name": "@nishin/reader",
	"version": "0.1.4",
	"description": "Read various data types from a byte array in a streamlined way",
	"keywords": [
		"binary",
		"data",
		"reader",
		"hex",
		"javascript",
		"typescript",
		"esm"
	],
	"license": "ISC",
	"author": "Tao Cumplido",
	"repository": "https://github.com/tao-cumplido/binary-reader/tree/main/packages/reader",
	"bugs": "https://github.com/tao-cumplido/binary-reader/issues",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.cjs"
		},
		"./data": {
			"import": "./dist/esm/data/index.js",
			"require": "./dist/cjs/data/index.cjs"
		},
		"./assert": {
			"import": "./dist/esm/assert.js",
			"require": "./dist/cjs/assert.cjs"
		}
	},
	"typesVersions": {
		"*": {
			"*": [
				"src/index.ts"
			],
			"data": [
				"src/data/index.ts"
			],
			"assert": [
				"src/assert.ts"
			]
		}
	},
	"scripts": {
		"build": "tsc",
		"test": "ava"
	},
	"dependencies": {
		"@nishin/enum": "~0.1.0"
	}
}
