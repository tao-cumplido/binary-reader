{
	"compilerOptions": {
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"useUnknownInCatchVariables": true,
		"noImplicitOverride": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"module": "esnext",
		"isolatedModules": true,
		"target": "esnext",
		"lib": ["esnext"],
		"sourceMap": true,
		"importHelpers": true,
		"composite": true
	},
	"exclude": ["node_modules"]
}
